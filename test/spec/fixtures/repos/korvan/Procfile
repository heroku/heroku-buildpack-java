web:      java -jar target/app.jar
callout:  java -cp target/app.jar Callout $(cat src/main/resources/callout-urls)
netpatch: java -cp target/app.jar NetPatch
jce:      java -cp target/app.jar JCE
https:    java -cp target/app.jar Https
ssl:      java -cp target/app.jar SSLTest httpbin.org 443
pgssl:    java -cp target/app.jar PostgresSSLTest
vmname:   java -cp target/app.jar VmName